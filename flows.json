    {
        "id": "534e80ecfb55cc6d",
        "type": "function",
        "z": "ea94d0040deba697",
        "name": "Subject Information",
        "func": "// ========================================\n// SUBJECT INFO PARSER FOR NODE-RED DASHBOARD\n// ========================================\n\nvar payload = msg.payload;\n\n// Validasi payload\nif (!payload || typeof payload !== 'object') {\n    return null;\n}\n\n// Siapkan array output untuk berbagai dashboard elements\nvar msgs = [];\n\n// ========================================\n// OUTPUT 1: Subject Name\n// ========================================\nmsgs[0] = {\n    payload: payload.subject_name || \"Unknown Subject\",\n    topic: \"subject_name\"\n};\n\n// ========================================\n// OUTPUT 2: Monitoring Status  \n// ========================================\nvar status = payload.status || \"unknown\";\nvar statusDisplay = \"\";\nvar statusColor = \"\";\n\nswitch(status) {\n    case \"monitoring_started\":\n        statusDisplay = \"ðŸŸ¢ MONITORING ACTIVE\";\n        statusColor = \"green\";\n        break;\n    case \"monitoring_stopped\":\n        statusDisplay = \"ðŸ”´ MONITORING STOPPED\";\n        statusColor = \"red\";\n        break;\n    default:\n        statusDisplay = \"âšª UNKNOWN STATUS\";\n        statusColor = \"gray\";\n}\n\nmsgs[1] = {\n    payload: statusDisplay,\n    topic: \"monitoring_status\",\n    color: statusColor\n};\n\n// ========================================\n// OUTPUT 3: Session Information\n// ========================================\nvar sessionInfo = \"\";\n\nif (payload.status === \"monitoring_started\") {\n    var startDate = new Date(payload.start_time * 1000);\n    sessionInfo = `Started: ${startDate.toLocaleString()}\\n`;\n    sessionInfo += `COM Port: ${payload.com_port || \"N/A\"}\\n`;\n    sessionInfo += `Directory: ${payload.save_directory || \"N/A\"}`;\n} else if (payload.status === \"monitoring_stopped\") {\n    var stopDate = new Date(payload.stop_time * 1000);\n    sessionInfo = `Stopped: ${stopDate.toLocaleString()}\\n`;\n    sessionInfo += `Total Iterations: ${payload.total_iterations || 0}\\n`;\n    sessionInfo += `Session Duration: ${calculateDuration(payload)}`;\n}\n\nmsgs[2] = {\n    payload: sessionInfo,\n    topic: \"session_info\"\n};\n\n// ========================================\n// OUTPUT 4: Current Time (for dashboard refresh)\n// ========================================\nmsgs[3] = {\n    payload: new Date().toLocaleString(),\n    topic: \"last_update\"\n};\n\n// ========================================\n// HELPER FUNCTION: Calculate Session Duration\n// ========================================\nfunction calculateDuration(data) {\n    if (data.start_time && data.stop_time) {\n        var duration = data.stop_time - data.start_time;\n        var minutes = Math.floor(duration / 60);\n        var seconds = Math.floor(duration % 60);\n        return `${minutes}m ${seconds}s`;\n    }\n    return \"N/A\";\n}\n\n// ========================================\n// OUTPUT 5: Complete Subject Data (for logging/storage)\n// ========================================\nmsgs[4] = {\n    payload: payload,\n    topic: \"complete_subject_data\"\n};\n\n// Debug log\nnode.warn(`Subject Info Update: ${payload.subject_name} - Status: ${payload.status}`);\n\nreturn msgs;",
